<odoo>
    <record id="set_channel_transport_wizard_form" model="ir.ui.view">
        <field name="name">Set Channel Transport Wizard</field>
        <field name="model">asterisk_plus.set_channel_transport_wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="transport"/>
                </group>
                <footer>
                    <button string="Submit" name="submit" type="object"
                            class="oe_highlight"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
